{
  "name": "@leny/timanti",
  "version": "0.0.1",
  "description": "A small pomodoro with React.",
  "private": true,
  "scripts": {
    "lint": "npx eslint --fix --cache src",
    "build": "npx parcel build src/index.html",
    "work": "npx parcel src/index.html --log-level 4"
  },
  "author": "leny <leny@becode.org>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leny/timanti.git"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@becode/eslint-config": "^0.9.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-unicorn": "^20.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.4",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5"
  },
  "lint-staged": {
    "*.json": [
      "npx prettier --write"
    ],
    "*.js": [
      "npx eslint --fix --cache"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  }
}
